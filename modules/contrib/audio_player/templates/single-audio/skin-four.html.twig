
 <div class="audio-main-container">
  {% for audio in audios %}
    {% set original_url = audio.original_url %}
    <div class="audio-player skin-four audio-player-container {{ palette }}">
        <audio class="audio-player-element" src="{{ original_url }}" preload="metadata"></audio>

        <div class="audio-player-wrapper">
            <div class="audio-player-controls audio-player-top-row">
                <div class="audio-player-play-pause-btn audio-player-control-btn">
                    <svg class="audio-player-icon audio-player-play-icon" viewBox="0 0 24 24">
                        <path d="M8 5v14l11-7z"/>
                    </svg>
                    <svg class="audio-player-icon audio-player-pause-icon" viewBox="0 0 24 24">
                        <path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/>
                    </svg>
                </div>

                <div class="audio-player-rewind-btn audio-player-control-btn">
                    <svg class="audio-player-icon" viewBox="0 0 24 24">
                        <path d="M11 18V6l-8.5 6 8.5 6zm.5-6l8.5 6V6l-8.5 6z"/>
                    </svg>
                </div>

                <div class="audio-player-fast-forward-btn audio-player-control-btn">
                    <svg class="audio-player-icon" viewBox="0 0 24 24">
                        <path d="M13 18V6l8.5 6-8.5 6zM3 6v12l8.5-6L3 6z"/>
                    </svg>
                </div>

                <div class="audio-player-song-info">
                    <span class="audio-player-song-name">Loading Song...</span>
                </div>

                <div class="audio-player-volume-controls">
                    <div class="audio-player-mute-unmute-btn audio-player-control-btn">
                        <svg class="audio-player-icon audio-player-volume-high-icon" viewBox="0 0 24 24">
                            <path d="M3 9v6h4l5 5V4L7 9H3zm13.5 3c0-1.77-1.02-3.29-2.5-4.03v8.05c1.48-.73 2.5-2.25 2.5-4.02zM14 3.23v2.06c2.89.86 5 3.54 5 6.71s-2.11 5.85-5 6.71v2.06c4.01-.91 7-4.49 7-8.77s-2.99-7.86-7-8.77z"/>
                        </svg>
                        <svg class="audio-player-icon audio-player-volume-mute-icon audio-player-hidden" viewBox="0 0 24 24">
                            <path d="M7 9v6h4l5 5V4l-5 5H7zm12.5-3.24L17.74 5 15 7.74 12.26 5 11 6.26 13.74 9 11 11.74 12.26 13 15 10.26 17.74 13 19 11.74 16.26 9 19 6.26z"/>
                        </svg>
                    </div>
                    <input type="range" class="audio-player-volume-slider" min="0" max="1" step="0.01" value="1">
                </div>
            </div>

            <div class="audio-player-controls audio-player-bottom-row">
                <span class="audio-player-current-time">00:00</span>
                <div class="audio-player-progress-container">
                    <div class="audio-player-buffered-bar"></div>
                    <div class="audio-player-played-progress-bar"></div>
                </div>
                <span class="audio-player-duration">00:00</span>
            </div>
        </div>
    </div>
  {% endfor %}
</div>
